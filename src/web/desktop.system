            if (hotkey !== undefined) {
                target.append(`<div class="${itemClass}" onclick="${functionName}(this,event);" payload="${payload}">
                        ${keyword}
                        <span class="description">${hotkey}</span>
                    </div>`);
            } else {
                target.append(`<div class="${itemClass}"  onclick="${functionName}(this,event);" payload="${payload}">
                        ${keyword}
                    </div>`);
            }
        }

