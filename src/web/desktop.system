            }else if (keycode == "67" && ctrlHold){
                //Ctrl + C, copy
                handleCopyOperation();
             }else if (keycode == "88" && ctrlHold){
                //Ctrl + X, cut
                handleCutOperation();
            }else if (keycode == "86" && ctrlHold){
                //Ctrl + V, Paste
                handlePasteOperation();
            }else{
                //console.log(event.which);
            }
        });
        
