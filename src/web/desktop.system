                //This windows do now allow resizing
                return;
            }
            var originalSize = [$(object).width(), $(object).height(), $(object).offset().left, $(object).offset().top];
            $(object).attr("orgsize", encodeURIComponent(JSON.stringify(originalSize)));
            $(object).css({
                width: "50%",
